          </outerBoundaryIs>
        </Polygon>
      </Placemark>

      <!-- Terreno 5 -->
      <Placemark>
        <name>Terreno Sul B</name>
        <description>Solo argiloso profundo</description>
        <Polygon>
          <outerBoundaryIs>
            <LinearRing>
              <coordinates>
                -7.8530,41.4980,238
                -7.8520,41.4980,240
                -7.8520,41.4990,242
                -7.8530,41.4990,240
                -7.8530,41.4980,238
              </coordinates>
            </LinearRing>
          </outerBoundaryIs>
        </Polygon>
      </Placemark>
    </Folder>
